{
  "name": "@dx/inject",
  "version": "0.1.17",
  "exports": {
    ".": "./mod.ts"
  },
  "tasks": {
    "lock": "deno cache ./**/*.ts"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1.0.3",
    "@std/testing": "jsr:@std/testing@1.0.1",
    "@npm/reflect-metadata": "npm:reflect-metadata@0.2.2"
  },
  "publish": {
    "exclude": [
      ".github",
      ".vscode",
      "examples",
      ".editorconfig",
      "deno.lock",
      "**/*.test.ts"
    ]
  },
  "compilerOptions": {
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  "fmt": {
    "useTabs": false,
    "lineWidth": 360,
    "indentWidth": 2,
    "singleQuote": true,
    "proseWrap": "preserve",
    "exclude": [
      ".vscode/**"
    ]
  }
}
