{
  "name": "@dx/inject",
  "version": "0.1.15",
  "exports": {
    ".": "./mod.ts"
  },
  "tasks": {
    "lock": "deno cache --lock=deno.lock ./**/*.ts"
  },
  "imports": {
    "@dx/reflect": "jsr:@dx/reflect@0.2.14",
    "@std/assert": "jsr:@std/assert@1.0.2",
    "@std/testing": "jsr:@std/testing@1.0.0"
  },
  "publish": {
