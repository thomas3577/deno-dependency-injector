{
  "name": "@dx/inject",
  "version": "0.1.3",
  "exports": {
    ".": "./mod.ts"
  },
  "imports": {
    "@reflect": "https://deno.land/x/deno_reflect@v0.2.1/mod.ts"
  },
  "compilerOptions": {
    "strict": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  "lint": {
    "rules": {
      "tags": [
        "recommended"
      ],
      "include": [
        "ban-untagged-todo",
        "no-deprecated-deno-api"
      ],
      "exclude": [
        "no-explicit-any",
        "no-inferrable-types"
      ]
    }
  },
  "fmt": {
    "useTabs": false,
    "lineWidth": 360,
    "indentWidth": 2,
    "singleQuote": true,
    "proseWrap": "preserve",
    "exclude": [
      ".vscode/**"
    ]
  }
}